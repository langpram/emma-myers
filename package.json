{
  "name": "tailwind",
  "module": "index.ts",
  "type": "module",
  "private": true,
  "scripts": {
    "tw": "bunx tailwindcss -i ./src/input.css -o ./dist/global.css --watch",
    "bulid-tw": "bunx tailwindcss -i ./src/input.css -o ./dist/global.css",
    "dev": "concurrently \"bun run tw\" \"bun build ./src/main.js --outdir ./dist\" \"bun run index.html\"",
    "build": "concurrently \"bun run bulid-tw\"\"bun build ./src/main.js --outdir ./dist\""
  },
  "dependencies": {
    "@tailwindcss/cli": "^4.1.8",
    "tailwindcss": "^4.1.8",
    "typed.js": "^2.1.0"
  },
  "devDependencies": {
    "concurrently": "^9.1.2"
  }
}
